{
  "name": "console-patterns",
  "description": "Collection of snippets to render logical patterns",
  "version": "3.0.0",
  "repository": "git@github.com:grikomsn/console-patterns.git",
  "author": "Griko Nibras <git@griko.id>",
  "scripts": {
    "build": "yarn build:dir && yarn build:favicons && next build",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node generate-favicons",
    "clean": "rimraf .next \"./public/!(social.png)\"",
    "deduplicate": "npx yarn-deduplicate && yarn",
    "dev": "next",
    "doctl": "doctl apps update 455f67d5-cf6f-45c0-8ee1-0c0d9391f7a6 --spec doctl.yaml",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "validate": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "@sindresorhus/class-names": "^1",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "autoprefixer": "^10",
    "copee": "^1",
    "favicons": "^6",
    "mkdirp": "^1",
    "next": "^10",
    "next-seo": "^4",
    "postcss": "^8",
    "postcss-flexbugs-fixes": "^5",
    "postcss-preset-env": "^6",
    "prism-react-renderer": "^1",
    "react": "^17",
    "react-dom": "^17",
    "react-live": "^2",
    "tailwindcss": "^2",
    "typescript": "^4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "eslint": "^7",
    "eslint-config-kentcdodds": "^17",
    "eslint-config-prettier": "^8",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "eslint-plugin-simple-import-sort": "^7",
    "husky": "^4",
    "lint-staged": "^10",
    "prettier": "^2",
    "rimraf": "^3"
  },
  "license": "MIT"
}
