{
  "name": "console-patterns",
  "description": "Collection of snippets to render logical patterns",
  "version": "4.0.0",
  "repository": "git@github.com:grikomsn/console-patterns.git",
  "author": "Griko Nibras <git@griko.id>",
  "scripts": {
    "# general commands": "##################################################",
    "build": "next build && yarn sitemap",
    "build:static": "yarn build && next export",
    "clean": "rimraf .eslintcache .next \"./public/!(social.png)\"",
    "deduplicate": "npx yarn-deduplicate && yarn",
    "dev": "next",
    "lint": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "prebuild": "mkdirp ./public/icons/ && node generate-favicons",
    "sitemap": "next-sitemap --config config/sitemap.js",
    "type-check": "tsc --noEmit",
    "validate": "yarn lint && yarn type-check",
    "#": "###################################################################"
  },
  "dependencies": {
    "@monaco-editor/react": "^4",
    "@sindresorhus/class-names": "^1",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "autoprefixer": "^10",
    "copee": "^1",
    "critters": "^0",
    "eslint": "^7",
    "eslint-config-kentcdodds": "^19",
    "eslint-config-next": "^11",
    "eslint-config-prettier": "^8",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "eslint-plugin-simple-import-sort": "^7",
    "favicons": "^6",
    "mkdirp": "^1",
    "next": "^11",
    "next-seo": "^4",
    "next-sitemap": "^1",
    "postcss": "^8",
    "postcss-flexbugs-fixes": "^5",
    "postcss-preset-env": "^6",
    "prism-react-renderer": "^1",
    "react": "^17",
    "react-dom": "^17",
    "react-live": "^2",
    "tailwindcss": "^2",
    "typescript": "^4",
    "zustand": "^3"
  },
  "devDependencies": {
    "husky": "^4",
    "lint-staged": "^11",
    "prettier": "^2",
    "rimraf": "^3"
  },
  "license": "MIT"
}
